{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "max-line-length": [
            true,
            140
        ],
        "member-access": [
            false
        ],
        "no-console": [
            false
        ],
        "no-var-requires": false,
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}
